{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "allowJs": false,
    "jsx": "preserve",
    "outDir": "./build",
    "isolatedModules": false,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true
  },
  "exclude": [],
  "include": [
    "src"
  ]
}
